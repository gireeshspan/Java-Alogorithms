package waitandnotify;

public class Job {
	
	public void produce(){
		
		try {
			System.out.println("Waiting for ket to press");
			wait();
			System.out.println("key pressed");
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
		
	}
	
	
	public void consumer(){
		
		System.out.println();
		
	}

}
